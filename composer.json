            "role": "Developer"
        },
	{
            "name": "Rodrigo Primo",
            "email": "rodrigosprimo@gmail.com",
            "role": "Developer"
        }	
    ],
    "require": {
        "composer/installers": "~1.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "^2.3"
    }
}
